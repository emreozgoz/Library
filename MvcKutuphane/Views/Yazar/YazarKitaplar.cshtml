@using MvcKutuphane.Models.Entity
@model List<TBLKİTAP>
@{
    ViewBag.Title = "YazarKitaplar";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Yazar Adı: @ViewBag.y1</h2>
    <br />
    <table class="table table-striped">
        <tr>
            <td>ID</td>
            <td>Kitap Adı</td>
            <td>Kategori</td>
            <td>Yayınevi</td>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.AD</td>
                <td>@item.TBLKATEGORİ.AD</td>
                <td>@item.YAYINEVİ</td>
            </tr>

        }
    </table>
